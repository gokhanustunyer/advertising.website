@using tatil.entity.Statics
@model SEO
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h4>Yeni SEO</h4>
<form class="mt-5 form-group" method="post">
  <h4 style="padding: .5rem">Email</h4>
    <div class="form-group mt-2">
        <label for="url" class="col-md-12">Url</label>
        <div class="col-md-12">
            <input type="text" name="Url" id="url" value="@Model.Url" class="form-control form-control-line">
        </div>
    </div>
    <div class="form-group mt-2">
        <label for="title" class="col-md-12">Başlık</label>
        <div class="col-md-12">
            <input type="text" name="Title" id="title" value="@Model.Title" class="form-control form-control-line">
        </div>
    </div>
    <div class="form-group mt-2">
        <label for="Description" class="col-md-12">Açıklama</label>
        <div class="col-md-12">
            <input type="text" name="Description" id="Description" value="@Model.Description" class="form-control form-control-line">
        </div>
    </div>
    <div class="form-group mt-2">
        <label for="Publisher" class="col-md-12">Publisher</label>
        <div class="col-md-12">
            <input type="text" name="Publisher" id="Publisher" value="@Model.Publisher" class="form-control form-control-line">
        </div>
    </div>
    <div class="form-group mt-2">
        <label for="Author" class="col-md-12">Author</label>
        <div class="col-md-12">
            <input type="text" name="Author" id="Author" value="@Model.Author" class="form-control form-control-line">
        </div>
    </div>
    <div class="form-group mt-2">
        <label for="Keywords" class="col-md-12">Keywords</label>
        <div class="col-md-12">
            <input type="text" name="Keywords" id="Keywords" value="@Model.Keywords" class="form-control form-control-line">
        </div>
    </div>
  <button class="btn btn-primary" style="width:100%;display:block">Kaydet</button>
</form>